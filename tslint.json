{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-string-literal": false,
        "no-console": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}